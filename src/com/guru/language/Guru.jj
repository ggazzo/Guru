String UnaryExpression() : {Token t1, t2; String tmp1="",tmp2;}
{
    (((t1 = "&" | t1 = "-" | t1 = "~" | t1 = "!"){tmp1+=t1.toString();  })* tmp2 = PrefixIncDecExpression())

    { return "%exp1% %exp2%".replace("%exp1%",tmp1.toString()).replace("%exp2%",tmp2).trim();  }
}

