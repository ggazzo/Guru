String AssignmentOperator() : {Token r;}
{
    ( r =  "=" | r = "+=" |r = "-=" | r = "*=" | r = "/=" | r = ".=" | r = "%=" | r = "&=" | r = "|=" | r = "^=" | r = "<<=" | r = ">>=" )
